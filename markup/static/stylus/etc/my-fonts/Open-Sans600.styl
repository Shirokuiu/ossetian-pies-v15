@font-face {
  font-family: 'Open Sans';
  font-style: normal;
  font-weight: 600;
  src: url('../fonts/open-sans-v15-cyrillic_latin-600.eot'); /* IE9 Compat Modes */
  src: local('Open Sans SemiBold'), local('OpenSans-SemiBold'),
       url('../fonts/Open-Sans/Open-Sans600/open-sans-v15-cyrillic_latin-600.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../fonts/Open-Sans/Open-Sans600/open-sans-v15-cyrillic_latin-600.woff2') format('woff2'), /* Super Modern Browsers */
       url('../fonts/Open-Sans/Open-Sans600/open-sans-v15-cyrillic_latin-600.woff') format('woff'), /* Modern Browsers */
       url('../fonts/Open-Sans/Open-Sans600/open-sans-v15-cyrillic_latin-600.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../fonts/Open-Sans/Open-Sans600/open-sans-v15-cyrillic_latin-600.svg#OpenSans') format('svg'); /* Legacy iOS */
}